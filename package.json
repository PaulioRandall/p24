{
	"author": "Paul Williams",
	"name": "p24",
	"description": "Simple tool for documenting Svelte components.",
	"type": "module",
	"license": "MIT",
	"version": "1.0.0",
	"engines": {
		"node": ">=18"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PaulioRandall/p24.git"
	},
	"keywords": [
		"documentation",
		"svelte",
		"plunder"
	],
	"exports": {
		"./package.json": "./package.json",
		".": "./src/index.js"
	},
	"bin": {
		"p24": "./src/render_readme_cli.js"
	},
	"scripts": {
		"fmt": "prettier --log-level warn --write .",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
		"commit": "npm run fmt && npm run test && cat ./ready-to-ship.txt"
	},
	"devDependencies": {
		"jest": "29.7.0",
		"prettier": "3.3.2",
		"upath": "2.0.1"
	},
	"dependencies": {
		"p23": "^1.0.1",
		"commander": "^12.1.0"
	}
}
